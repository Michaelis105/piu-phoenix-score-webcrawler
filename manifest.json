{
  "manifest_version": 3,
  "name": "PIU Score Exporter",
  "version": "1.1",
    "description": "Exports scores from piugame.com to CSV",
  "permissions": ["scripting", "activeTab"],
  "host_permissions": ["https://piugame.com/*"],
  "background": {
    "scripts": ["background.js"]
  },
  "action": {"default_title": "Run Score Export"}
}